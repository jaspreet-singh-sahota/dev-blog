<p id="notice"><%= notice %></p>

<h1>Categoriesses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categoriesses.each do |categoriess| %>
      <tr>
        <td><%= link_to 'Show', categoriess %></td>
        <td><%= link_to 'Edit', edit_categoriess_path(categoriess) %></td>
        <td><%= link_to 'Destroy', categoriess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categoriess', new_categoriess_path %>
